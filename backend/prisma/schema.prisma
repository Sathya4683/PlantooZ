generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
}


model Items {
  item_id   Int    @id @default(autoincrement())
  item_name String
  item_price Decimal  @db.Decimal(10, 2)
}
